  "dependencies": {
    "express": "^4.13.4",
    "http": "0.0.0",
    "mongoose": "^4.4.19"
  },
  "engines": {
    "node": "=6.*.*"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "chai": "^3.5.0",
